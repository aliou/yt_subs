<div class='container'>
  <div class='col-md-6 col-md-push-3'>
    <h2>List of channels</h2>
    <ul class='list-group'>
      <% @channels.each do |channel| %>
        <li class='list-group-item'>
          <%= link_to channel.title, channel %>
        </li>
      <% end %>
    </ul>
  </div>
</div>

<%= link_to 'New Channel', new_channel_path %>
